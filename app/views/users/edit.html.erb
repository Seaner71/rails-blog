<h1><%= @user.username %></h1>


<%= form_with(model: @user, local: true) do |f| %>

<div class="form-group">
  <%= f.label :username %>
  <%= f.text_field :username %>
</div>

<div class="form-group">
  <%= f.label :email %>
  <%= f.email_field :email %>
</div>

<div class="form-group">
  <%= f.submit "Edit"%>
</div>
<% end %>

<%= link_to 'Back', user_path %>



  <%= link_to 'Delete', @user, method: 'delete', local: true %>
